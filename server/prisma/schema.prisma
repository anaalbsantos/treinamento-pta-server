// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement())
  firstName String
  lastName  String
  age       Int
}

model Test {
  id   Int    @id @default(autoincrement())
  name String
  age  Int
}

model Notebook {
  id          Int    @id @default(autoincrement())
  name        String
  description String?
  price       Float
  color       String
  stock       Int
  createdAt   DateTime @default(now())
}
